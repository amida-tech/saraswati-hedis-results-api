{
  "name": "saraswati-hedis-results-api",
  "version": "1.0.1",
  "description": "An API that ingests and renders HEDIS data",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env node src/index.js",
    "develop": "cross-env nodemon src/index.js",
    "debug": "cross-env nodemon --inspect-brk=9999 src/index.js",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "test": "cross-env jest --silent --runInBand --coverage --silent=false",
    "docker:build": "docker build -t saraswati-hedis-results-api .",
    "docker:run": "docker run -p 3000:3000 saraswati-hedis-results-api"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "jest": {
    "coverageReporters": [
      "cobertura"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/build"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-validation": "^4.1.0",
    "express-winston": "^4.2.0",
    "fast-xml-parser": "^4.2.4",
    "http-status": "^1.6.2",
    "jsonwebtoken": "^9.0.0",
    "kafkajs": "^2.2.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongodb": "^5.5.0",
    "node-cron": "^3.0.2",
    "pg": "^8.11.0",
    "prom-client": "14",
    "regenerator-runtime": "^0.13.11",
    "sequelize": "^6.31.1",
    "supertest": "^6.3.3",
    "supertest-as-promised": "^4.0.2",
    "swagger-stats": "^0.99.5",
    "winston": "^3.8.2",
    "winston-json-formatter": "^0.10.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "minimist": "^1.2.8",
    "nodemon": "^2.0.22",
    "uuid": "^9.0.0"
  }
}